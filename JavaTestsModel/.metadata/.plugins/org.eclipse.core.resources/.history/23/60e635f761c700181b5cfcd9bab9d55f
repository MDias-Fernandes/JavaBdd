package stepsDefinition;

import cucumber.api.java.en.Then;
import cucumber.api.java.en.When;
import pages.*;

public class ConsultoriaCadastro {
	
	public static String geraCPF() {  
        String iniciais = "";  
        Integer numero;  
        for (int i = 0; i < 9; i++) {  
            numero = new Integer((int) (Math.random() * 10));  
            iniciais += numero.toString();  
        }  
        return iniciais + calcDigVerif(iniciais);  
    }
	
	@When("^clico no botao cadastro$")
	public void clico_no_botao_cadastro() throws Throwable {
	    ConsultoriaCadastroPages.newRegister();
	}

	@When("^insiro um cpf valido$")
	public void insiro_um_cpf_valido() throws Throwable {
	    ConsultoriaCadastroPages.setCpf("");
	}

	@When("^insiro um nome valido$")
	public void insiro_um_nome_valido() throws Throwable {
	    
	}

	@When("^seleciono um genero$")
	public void seleciono_um_genero() throws Throwable {
	    
	}

	@When("^insiro uma data de nascimento$")
	public void insiro_uma_data_de_nascimento() throws Throwable {
	    
	}

	@When("^insiro um email valido$")
	public void insiro_um_email_valido() throws Throwable {
	    
	}

	@When("^insiro um telefone valido$")
	public void insiro_um_telefone_valido() throws Throwable {
	    
	}

	@When("^insiro um CEP comum$")
	public void insiro_um_CEP_comum() throws Throwable {
	    
	}

	@Then("^valido estrutura de validacao$")
	public void valido_estrutura_de_validacao() throws Throwable {
	    
	}
	
}
